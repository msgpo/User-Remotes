<?xml version="1.0" encoding="utf-8"?>
<layout>
  <grid>
    <row>
      <button id="play_prev" icon="ARROWLEFT" color="red" onTap="play_prev" />
      <button id="play_next" icon="ARROWRIGHT" color="red" onTap="play_next" />
    </row>
    <row>
      <button id="fullscreen" icon="FULLSCREEN" color="red" onTap="fullscreen" />
      <button id="play_all" icon="PLAY" color="red" onTap="play_all" />
      <button id="quit" icon="STOP" color="red" onTap="quit" />
    </row>
    <row>
      <button id="rotate_left" icon="ROTATELEFT" color="red" onTap="rotate_left" />
      <button id="rotate_right" icon="ROTATERIGHT" color="red" onTap="rotate_right" />
      <button id="delete" icon="BACKSPACE" color="red" onTap="img_delete" />
    </row>
    <row>
      <button id="zoom_out" icon="ZOOMOUT" color="red" onTap="zoom_out" />
      <button id="zoom_normal" icon="ZOOMNORMAL" color="red" onTap="zoom_normal" />
      <button id="zoom_in" icon="ZOOMIN" color="red" onTap="zoom_in" />
    </row>
    <row>
      <button id="cur_left" icon="LEFT" color="red" onTap="cur_left" />
      <button id="cur_right" icon="RIGHT" color="red" onTap="cur_right" />
      <button id="cur_up" icon="UP" color="red" onTap="cur_up" />
      <button id="cur_down" icon="DOWN" color="red" onTap="cur_down" />
    </row>
  </grid>
</layout>